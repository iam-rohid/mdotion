{
  "name": "web-app",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\" --port 3030",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "clean": "rimraf node_modules && rimraf .turbo && rimraf .cache"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/vercel": "^1.6.7",
    "@vercel/node": "^2.4.4",
    "classnames": "^2.3.1",
    "jotai": "^1.7.6",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.34.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7",
    "tsconfig": "*",
    "typescript": "^4.7.4"
  }
}
